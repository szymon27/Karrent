<Window x:Class="Karrent.Views.SignInWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Karrent.Views"
        mc:Ignorable="d"
        Title="SignInWindow" Height="277.366" Width="180.818" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label x:Name="lblUsername" Content="Username" HorizontalAlignment="Left" Margin="25,21,0,0" VerticalAlignment="Top" Width="120" Height="23"/>
        <TextBox x:Name="txtUsername" HorizontalAlignment="Left" Height="23" Margin="25,45,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="passPassword" HorizontalAlignment="Left" Margin="25,97,0,0" VerticalAlignment="Top" Width="120" Height="23"/>
        <Label x:Name="lblPassword" Content="Password" HorizontalAlignment="Left" Margin="25,76,0,0" VerticalAlignment="Top" Width="120" Height="26"/>
        <Button x:Name="btnSignIn" Content="SIGN IN" HorizontalAlignment="Left" Margin="25,147,0,0" VerticalAlignment="Top" Width="120" Height="44" Click="btnSignIn_Click"/>
        <Button x:Name="btnSignUp" Content="SIGN UP" HorizontalAlignment="Left" Margin="25,200,0,0" VerticalAlignment="Top" Width="120" Height="44" Click="btnSignUp_Click"/>
    </Grid>
</Window>
